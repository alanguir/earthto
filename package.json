{
  "name": "earth-to",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode=development",
    "lint": "vue-cli-service lint",
    "dev": "concurrently \"vue-cli-service build --mode=development\" \"nodemon index.js\""
  },
  "dependencies": {
    "bella-scheduler": "^1.2.2",
    "concurrently": "^5.2.0",
    "cuid": "^1.3.8",
    "express": "^4.14.1",
    "lodash": "^4.17.15",
    "picnic": "^6.5.0",
    "socket.io": "^2.4.0",
    "solaris-model": "^0.1.0",
    "vue": "^2.5.16",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.9",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.9",
    "@vue/cli-plugin-router": "^4.4.1",
    "@vue/cli-plugin-vuex": "^4.4.1",
    "@vue/cli-service": "^3.0.0-beta.9",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "nodemon": "^2.0.4",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "design/*"
    ],
    "delay": "1000"
  }
}
